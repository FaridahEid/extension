{
  "manifest_version": 3,
  "name": "Test extension",
  "version": "1.0",
  "description": "Hello world extension",
  "icons":{
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "background": {
    "service_worker": "popup.js"
  },
  "action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}